{
  "name": "@bumble/rxjs-log",
  "version": "1.0.0",
  "description": "Easily log RxJS Observables to the console with this operator",
  "repository": "jacksteamdev/rxjs-log",
  "keywords": [
    "rxjs",
    "observable",
    "console",
    "debug"
  ],
  "license": "MIT",
  "author": "Jack and Amy Steam <jacksteamdev@gmail.com>",
  "files": [
    "types",
    "dist"
  ],
  "main": "build/bundle-cjs.js",
  "module": "build/bundle-esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc -p tsconfig.d.json",
    "start": "rollup -c -w",
    "test": "jest",
    "preversion": "npm run build",
    "postversion": "git push && npm publish"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ],
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "dependencies": {
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "rollup": "^1.24.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  }
}
